    "antd": "^4.15.6",
    "aws-sdk": "^2.788.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "jimp": "^0.16.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "sequelize": "^6.6.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "lint": "^0.7.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1"
  }
