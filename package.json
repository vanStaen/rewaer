{
  "name": "rewaer",
  "version": "0.1.0",
  "description": "Rewaer App",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --write '**/*.{html,js,jsx,ts,tsx,css,json,graphql,graphqls}'",
    "start": "node index.js",
    "start-client": "react-scripts start",
    "devfr": "cd frontend/; npm i; npm start; cd ../;",
    "dev": "npm i; nodemon  index.js",
    "version": "heroku --version; git --version",
    "buildfe": "cd frontend/; npm i; npm run build; cd ../; git add -A; git commit -a -m \"deploy frontend Build\"; git push;",
    "deploy": "npm i; npm run backup; npm run buildfe; heroku git:remote -a rewaer; git push heroku master",
    "log": "heroku logs --tail",
    "test": "cd frontend; jest; cd ../;",
    "backup": "cd ressources/; node createDatabaseBackUpFile.js; cd ../; git add -A; git commit -a -m \"database backup\"; git push;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanStaen/rewaer.com.git"
  },
  "keywords": [
    "rewear",
    "fashionRevolution",
    "lookbook",
    "app"
  ],
