  "scripts": {
    "start": "node server/index.js",
    "devfr": "webpack serve --config webpack.config.dev.js --hot --open",
    "dev": "nodemon  server/index.js",
    "prebuild": "rm -rf ./build",
    "build": "webpack --config webpack.config.prod.js --mode production",
    "deploy": "npm i; heroku git:remote -a rewaer; git push heroku master; npm run backup;",
    "log": "heroku logs --tail",
    "backup": "cd server/lib/; node createDatabaseBackUpFile.js; npm run pushbackup; cd ../../",
    "pushbackup": "cd ../database-backups; git pull; git add -A; git commit -m 'Rewaer - database backup files'; git push; cd ../rewaer"
  },
