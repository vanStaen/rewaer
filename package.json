{
  "name": "rewaer",
  "version": "1.0.0",
  "description": "Rewaer App",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanStaen/rewaer.git"
  },
  "keywords": [
    "rewear",
    "fashionRevolution",
    "lookbook",
    "app"
  ],
  "author": "ClÃ©ment van Staen",
  "homepage": "https://github.com/vanStaen/rewaer#readme",
  "scripts": {
    "start": "node server/index.js",
    "dev": "run-p dev:frontend dev:backend",
    "dev:frontend": "webpack serve --config webpack.config.dev.cjs --hot --open",
    "dev:backend": "nodemon  server/index.js",
    "prebuild": "rm -rf ./build",
    "build": "webpack --config webpack.config.prod.cjs --mode production",
    "deploy": "npm i --force; npm run lint --fix; npm run test; heroku git:remote -a rewaer; git push heroku master;",
    "log": "heroku logs --tail",
    "backup": "cd server/lib/; node createDatabaseBackUpFile.js; cd ../../",
    "lint": "npm run style:css && npm run prettier:less && npm run lint:js",
    "lint:js": "eslint -c ./.eslintrc.json '**/*.js' '**/*.jsx' '**/*.ts' '**/*.tsx' --fix",
    "style:css": "stylelint '**/*.less' '**/*.css' --fix",
    "prettier:less": "prettier --write '**/*.less' '**/*.css'",
    "pushbackup": "cd ../database-backups; git pull; git add -A; git commit -m 'Rewaer - database backup files'; git push; cd ../rewaer",
    "test": "jest"
  },
